<!doctype html>
<!--[if lt IE 9]><html class="no-js no-svg ie lt-ie9 lt-ie8 lt-ie7" {{ site.language_attributes }}> <![endif]-->
<!--[if IE 9]><html class="no-js no-svg ie ie9 lt-ie9 lt-ie8" {{ site.language_attributes }}> <![endif]-->
<!--[if gt IE 9]><!-->
	<html
	class="no-js no-svg" {{ site.language_attributes }}
	style="visibility: hidden;"> <!--<![endif]-->
	{% block html_head_container %}
		<head>
			{{ function('wp_head') }}
			{% do action('taoti_do_css') %}

			<!-- Google Tag Manager -->
				{% include 'partial/gtm-head.twig' %}
			<!-- End Google Tag Manager -->
			<meta http-equiv="X-UA-Compatible" content="IE=edge">
			<meta name="viewport" content="width=device-width, initial-scale=1">

			<link rel="apple-touch-icon" sizes="180x180" href="{{ theme.link }}/images/favicons/apple-touch-icon.png"/>
			<link rel="icon" type="image/x-icon" href="{{ theme.link }}/images/favicons/favicon.ico">
			<link rel="icon" type="image/png" sizes="32x32" href="{{ theme.link }}/images/favicons/favicon-32x32.png">
			<link rel="icon" type="image/png" sizes="16x16" href="{{ theme.link }}/images/favicons/favicon-16x16.png">
			<link rel="manifest" href="{{ theme.link }}/site.webmanifest" />

			{# Riva Custom Font #}
			{# <link rel="stylesheet" href="https://use.typekit.net/xgy3myr.css"> #}

			{# Google icons #}
			{# <link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Material+Symbols+Outlined:opsz,wght,FILL,GRAD@48,400,0,0" /> #}

			<!--script src="https://cdn.jsdelivr.net/npm/typed.js@2.0.12"></script>

			<script src="https://cdnjs.cloudflare.com/ajax/libs/gsap/3.10.4/gsap.min.js"></script>
			<script src="https://cdnjs.cloudflare.com/ajax/libs/gsap/3.10.4/ScrollTrigger.min.js"></script-->
			<link rel="preconnect" href="https://fonts.googleapis.com">
			<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
			<link href="https://fonts.googleapis.com/css2?family=Kumbh+Sans:wght@300;400;600;700;900&display=swap" rel="stylesheet">
		</head>
	{% endblock %}

	<body class="{{body_class}} riva-styles" data-template="base.twig">
		<div class="heavy-background"></div>
		{# End: heavy-backgorund #}

		 <!-- Google Tag Manager (noscript) -->
			{% include 'partial/gtm-body.twig' %}
		<!-- End Google Tag Manager (noscript) -->

		<a class="visually-hidden-focusable" href="#content">{{ _e( 'Skip to content') }}</a>

		{# Add brandbar style components here. #}

		{% block header %}
			{% include 'partial/header.twig' with {"menu_item": menu.get_items} %}
		{% endblock %}

		{# Main Content #}
		<main id="content" class="content-wrapper">
			<div class="wrapper {{ sidebar_class }}">
				{% block content %}
					Sorry, no content
				{% endblock %}
			</div>
			{% if sidebar %}
				<aside class="layout-sidebar">
					{{ sidebar }}
				</aside>
			{% endif %}
		</main>

		{% block footer %}
			{% include 'partial/footer.twig' %}
		{% endblock %}

		<noscript><style>html { visibility: visible; }</style></noscript> {# FOUC fallback for browsers without js #}
	</body>

</html>
